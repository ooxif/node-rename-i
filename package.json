    "fix:json": "prettier --write \"**/*.json\"",
    "fix:ts": "tslint --fix --format verbose --project .",
    "lint": "run-p -cs lint:js lint:json lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .js --ext .cjs .",
    "lint:json": "prettier -l \"**/*.json\"",
    "lint:ts": "tslint --format verbose --project .",
    "prebuild": "rimraf dist",
